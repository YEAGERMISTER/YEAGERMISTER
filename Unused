#Lists/Tuples/Sets/Dctionaries``

Name = (1,2,3,4,)

L = [
    {"Name":"Harry","House":"Gryffindor"},
     {"Name":"Draco","House":"Slytherin"},
     {"Name":"Hermione","House":"Ravenclaw"},
]

def sort_items(person):
    return person["House"]

L.sort(key=sort_items)

class Point():
    def __init__(self,input1,input2):
        self.x=input1
        self.y=input2 
    
H = Point(-1,50)

print(H.x)
print(H.y)